<h1>Edit <%=@landmark.name%></h1>

<form action="/landmarks/<%= @landmark.id %>" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">

  <label> Landmark Name </label>
  <input type="text" name="name" value='<%= @landmark.name %>' id='landmark[name]'> </br>

  <label> Year Completed </label>
  <input type="text" name="year_completed" value='<%= @landmark.year_completed %>' id="landmark[year_completed]"> </br>

  <input type="submit" value="Edit Landmark">
</form>
